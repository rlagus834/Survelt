<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
 
 <mapper namespace="Project"> 
 	<select id="idCheck" parameterType="String" resultType="String">
 	SELECT ID FROM CLIENTTAB WHERE ID=#{id}
 	</select>
 	<insert id="MemberJoin" parameterType="member">
 	INSERT INTO CLIENTTAB VALUES(#{id},#{password},#{name},#{birth},#{email},#{address},#{phone},#{files})
 	</insert>
 	<select id="LoginCheck" parameterType="HashMap" resultType="String">
 	SELECT ID FROM CLIENTTAB WHERE ID=#{id} AND PASSWORD=#{password}
 	</select>	
 </mapper>
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 
 